# H003: Unused Trait
# Trait is defined but never referenced by any profile

project "H003 Test" {
    start: 2025-01-06
    currency: USD
}

trait senior {
    description: "Senior level"
    rate_multiplier: 1.3
}

trait contractor {
    description: "External contractor"
    rate_multiplier: 1.2
}

# Profile only uses 'senior', not 'contractor'
resource_profile senior_developer "Senior Developer" {
    traits: [senior]
    rate: {
        min: 100
        max: 150
    }
}

task development "Development" {
    effort: 5d
    assign: senior_developer
}
