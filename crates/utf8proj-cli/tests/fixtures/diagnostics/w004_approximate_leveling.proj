# W004: Approximate Leveling
# Resource leveling cannot fully resolve all conflicts

project "W004 Test" {
    start: 2025-01-06
    currency: USD
}

resource alice "Alice" {
    rate: 100/day
    capacity: 1.0
}

# Three parallel tasks all assigned to alice
# with a hard deadline that prevents full resolution

task task_a "Task A" {
    effort: 5d
    assign: alice
}

task task_b "Task B" {
    effort: 5d
    assign: alice
}

task task_c "Task C" {
    effort: 5d
    assign: alice
    must_finish_on: 2025-01-10
}
