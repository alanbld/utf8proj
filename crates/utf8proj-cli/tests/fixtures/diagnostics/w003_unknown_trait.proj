# W003: Unknown Trait
# Profile references a trait that is not defined

project "W003 Test" {
    start: 2025-01-06
    currency: USD
}

trait senior {
    description: "Senior level"
    rate_multiplier: 1.3
}

# Note: 'onsite' trait is NOT defined

resource_profile senior_developer "Senior Developer" {
    traits: [senior, onsite]
    rate: {
        min: 100
        max: 150
    }
}

resource alice "Alice" {
    rate: 120/day
}

task development "Development" {
    effort: 5d
    assign: alice
}
